{
  "name": "logstf-parser",
  "version": "1.0.0",
  "description": "TF2 log parser",
  "main": "./lib/cjs/LogParser.js",
  "module": "./lib/esm/LogParser.js",
  "exports": {
    "import": "./lib/esm/LogParser.js",
    "require": "./lib/cjs/LogParser.js"
  },
  "scripts": {
    "buildcjs": "tsc --project tsconfig.cjs.json",
    "buildesm": "tsc",
    "build": "tsc  && tsc --project tsconfig.cjs.json",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:ci": "jest --ci --maxWorkers 4"
  },
  "keywords": [
    "logs.tf",
    "tf2"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TheBv/logstf-parser.git"
  },
  "files": [
    "lib/"
  ],
  "author": "zoob",
  "maintainers": [
    "Bv"
  ],
  "license": "AGPL",
  "dependencies": {
    "xregexp": "^4.4.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.10",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  }
}
